<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.ebf" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>EBF</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <engine name="browser" spec="~5.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <allow-navigation href="*" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <splash density="xxxhdpi" height="1920" platform="android" src="res/screen/android/screen-xxxhdpi-port.png" width="1280" />
    <splash density="xxxhdpi" height="1280" platform="android" src="res/screen/android/screen-xxxhdpi-land.png" width="1920" />
    <splash density="xxhdpi" height="1600" platform="android" src="res/screen/android/screen-xxhdpi-port.png" width="960" />
    <splash density="xxhdpi" height="960" platform="android" src="res/screen/android/screen-xxhdpi-land.png" width="1600" />
    <splash density="xhdpi" height="960" platform="android" src="res/screen/android/screen-xhdpi-port.png" width="640" />
    <splash density="xhdpi" height="640" platform="android" src="res/screen/android/screen-xhdpi-land.png" width="960" />
    <splash density="mdpi" height="480" platform="android" src="res/screen/android/screen-mdpi-port.png" width="320" />
    <splash density="mdpi" height="320" platform="android" src="res/screen/android/screen-mdpi-land.png" width="480" />
    <splash density="hdpi" height="640" platform="android" src="res/screen/android/screen-hdpi-port.png" width="480" />
    <splash density="hdpi" height="480" platform="android" src="res/screen/android/screen-hdpi-land.png" width="800" />
    <splash density="ldpi" height="320" platform="android" src="res/screen/android/screen-ldpi-port.png" width="240" />
    <splash density="ldpi" height="240" platform="android" src="res/screen/android/screen-ldpi-land.png" width="400" />

    <splash platform="ios" src="res/screen/ios/Default~iphone.png" width="320" height="480"/>
    <splash platform="ios" src="res/screen/ios/Default@2x~iphone.png" width="640" height="960"/>
    <splash platform="ios" src="res/screen/ios/Default-568h@2x~iphone.png" width="640" height="1136"/>
    <splash platform="ios" src="res/screen/ios/Default-667h.png" width="750" height="1334"/>
    <splash platform="ios" src="res/screen/ios/Default-736h.png" width="1242" height="2208"/>
    <splash src="res/screen/ios/Default@2x~universal~anyany.png" />

    <icon platform="android" src="res/screen/android/icon-ldpi.png" density="ldpi" />
    <icon platform="android" src="res/screen/android/icon-mdpi.png" density="mdpi" />
    <icon platform="android" src="res/screen/android/icon-hdpi.png" density="hdpi" />
    <icon platform="android" src="res/screen/android/icon-xhdpi.png" density="xhdpi" />
    <icon platform="android" src="res/screen/android/icon-xxhdpi.png" density="xxhdpi" />
    <icon platform="android" src="res/screen/android/icon-xxxhdpi.png" density="xxxhdpi" />


    <!-- iOS 8.0+ -->
    <!-- iPhone 6 Plus  -->
    <icon platform="ios" src="res/screen/ios/Icon-60@3x.png" width="180" height="180" />
    <!-- iOS 7.0+ -->
    <!-- iPhone / iPod Touch  -->
    <icon platform="ios" src="res/screen/ios/Icon-60.png" width="60" height="60" />
    <icon platform="ios" src="res/screen/ios/Icon-60@2x.png" width="120" height="120" />
    <!-- iPad -->
    <icon platform="ios" src="res/screen/ios/Icon-76.png" width="76" height="76" />
    <icon platform="ios" src="res/screen/ios/Icon-76@2x.png" width="152" height="152" />
    <!-- Spotlight Icon -->
    <icon platform="ios" src="res/screen/ios/Icon-40.png" width="40" height="40" />
    <icon platform="ios" src="res/screen/ios/Icon-40@2x.png" width="80" height="80" />
    <!-- iOS 6.1 -->
    <!-- iPhone / iPod Touch -->
    <icon platform="ios" src="res/screen/ios/Icon.png" width="57" height="57" />
    <icon platform="ios" src="res/screen/ios/Icon@2x.png" width="114" height="114" />
    <!-- iPad -->
    <icon platform="ios" src="res/screen/ios/Icon-72.png" width="72" height="72" />
    <icon platform="ios" src="res/screen/ios/Icon-72@2x.png" width="144" height="144" />
    <!-- iPad Pro -->
    <icon platform="ios" src="res/screen/ios/Icon-167.png" width="167" height="167" />
    <!-- iPhone Spotlight and Settings Icon -->
    <icon platform="ios" src="res/screen/ios/Icon-small.png" width="29" height="29" />
    <icon platform="ios" src="res/screen/ios/Icon-small@2x.png" width="58" height="58" />
    <!-- iPad Spotlight and Settings Icon -->
    <icon platform="ios" src="res/screen/ios/Icon-50.png" width="50" height="50" />
    <icon platform="ios" src="res/screen/ios/Icon-50@2x.png" width="100" height="100" />
    <!-- iPad Pro -->
    <icon platform="ios" src="res/screen/ios/Icon-83.5@2x.png" width="167" height="167" />




    <plugin name="cordova-plugin-inappbrowser" spec="~2.0.2" />
</widget>
