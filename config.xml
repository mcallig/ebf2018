<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.ebf" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>EBF</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <engine name="browser" spec="~5.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <allow-navigation href="*" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="ShowSplashScreenSpinner" value="false"/>
    <splash platform="android" density="xxxhdpi" src="res/screen/android/screen-xxxhdpi-port.png" width="1280" height="1920" />

    <splash platform="android" density="xxxhdpi" src="res/screen/android/screen-xxxhdpi-land.png" width="1920" height="1280" />

    <splash platform="android" density="xxhdpi" src="res/screen/android/screen-xxhdpi-port.png" width="960" height="1600" />

    <splash platform="android" density="xxhdpi" src="res/screen/android/screen-xxhdpi-land.png" width="1600" height="960" />

    <splash platform="android" density="xhdpi" src="res/screen/android/screen-xhdpi-port.png" width="640" height="960" />

    <splash platform="android" density="xhdpi" src="res/screen/android/screen-xhdpi-land.png" width="960" height="640" />

    <splash platform="android" density="mdpi" src="res/screen/android/screen-mdpi-port.png" width="320" height="480" />

    <splash platform="android" density="mdpi" src="res/screen/android/screen-mdpi-land.png" width="480" height="320" />

    <splash platform="android" density="hdpi" src="res/screen/android/screen-hdpi-port.png" width ="480" height="640" />

    <splash platform="android" density="hdpi" src="res/screen/android/screen-hdpi-land.png" width ="800" height="480" />

    <splash platform="android" density="ldpi" src="res/screen/android/screen-ldpi-port.png" width="240" height="320" />

    <splash platform="android" density="ldpi" src="res/screen/android/screen-ldpi-land.png" width="400" height="240" />
</widget>
